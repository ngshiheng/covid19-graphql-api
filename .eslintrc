{
    "rules": {
        "@typescript-eslint/naming-convention": ["error"]
    },
    "parser": "@typescript-eslint/parser",
    "plugins": ["graphql", "@typescript-eslint"],
    "env": {
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    }
}